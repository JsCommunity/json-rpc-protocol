{
  "name": "json-rpc-protocol",
  "version": "0.10.0",
  "license": "ISC",
  "description": "JSON-RPC 2 protocol messages parsing and formatting",
  "keywords": [
    "json",
    "json-rpc",
    "jsonrpc",
    "jsonrpc2",
    "rpc"
  ],
  "homepage": "https://github.com/julien-f/js-json-rpc-protocol",
  "bugs": "https://github.com/julien-f/js-json-rpc-protocol/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-f/js-json-rpc-protocol.git"
  },
  "author": {
    "name": "Julien Fontanet",
    "email": "julien.fontanet@isonoe.net"
  },
  "preferGlobal": false,
  "main": "dist/",
  "bin": {},
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "lodash": "^4.15.0",
    "make-error": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "clarify": "^1.0.5",
    "dependency-check": "^2.5.1",
    "ghooks": "^1.2.1",
    "mocha": "^3.0.0",
    "must": "^0.13.1",
    "nyc": "^8.1.0",
    "source-map-support": "^0.4.0",
    "standard": "^8.0.0",
    "trace": "^2.0.0"
  },
  "scripts": {
    "build": "NODE_ENV=production babel --source-maps --out-dir=dist/ src/",
    "depcheck": "dependency-check ./package.json",
    "dev": "babel --watch --source-maps --out-dir=dist/ src/",
    "dev-test": "mocha --opts .mocha.opts --watch --reporter=min \"dist/**/*.spec.js\"",
    "lint": "standard",
    "posttest": "npm run lint && npm run depcheck",
    "prepublish": "npm run build",
    "test": "nyc mocha --opts .mocha.opts \"dist/**/*.spec.js\""
  },
  "config": {
    "ghooks": {
      "commit-msg": "npm test"
    }
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ]
  },
  "standard": {
    "ignore": [
      "dist/**"
    ],
    "parser": "babel-eslint"
  }
}
