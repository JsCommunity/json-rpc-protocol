{
  "name": "json-rpc-protocol",
  "version": "0.10.0",
  "license": "ISC",
  "description": "JSON-RPC 2 protocol messages parsing and formatting",
  "keywords": [
    "json",
    "json-rpc",
    "jsonrpc",
    "jsonrpc2",
    "rpc"
  ],
  "homepage": "https://github.com/julien-f/js-json-rpc-protocol",
  "bugs": "https://github.com/julien-f/js-json-rpc-protocol/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/julien-f/js-json-rpc-protocol.git"
  },
  "author": {
    "name": "Julien Fontanet",
    "email": "julien.fontanet@isonoe.net"
  },
  "preferGlobal": false,
  "main": "dist/",
  "bin": {},
  "files": [
    "dist/"
  ],
  "dependencies": {
    "babel-runtime": "^5.4.7",
    "lodash.isarray": "^3.0.3",
    "lodash.isnumber": "^3.0.1",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^3.0.1",
    "lodash.map": "^3.1.4",
    "make-error": "^1.0.2"
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-eslint": "^4.1.3",
    "clarify": "^1.0.5",
    "mocha": "^2.2.5",
    "must": "^0.13.1",
    "source-map-support": "^0.3.2",
    "standard": "^7.0.0",
    "trace": "^2.0.0"
  },
  "scripts": {
    "build": "babel --source-maps --out-dir=dist/ src/",
    "dev": "babel --watch --source-maps --out-dir=dist/ src/",
    "lint": "standard",
    "posttest": "npm run lint",
    "prepublish": "npm run build",
    "test": "mocha --opts .mocha.opts \"dist/**/*.spec.js\"",
    "test-dev": "mocha --opts .mocha.opts --watch --reporter=min \"dist/**/*.spec.js\""
  },
  "standard": {
    "ignore": [
      "dist/**"
    ],
    "parser": "babel-eslint"
  }
}
